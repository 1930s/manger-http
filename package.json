{
  "name": "manger-http",
  "version": "1.0.2",
  "description": "HTTP API for manger",
  "main": "index.js",
  "scripts": {
    "start": "node start.js > /dev/null & echo $! > .pid",
    "stop": "kill `< .pid`",
    "debug": "NODE_DEBUG=1 node start.js | bunyan -o short",
    "test": "NODE_TEST=1 tap -b test/*.js",
    "posttest": "rm -rf /tmp/manger-http-[1-9]*",
    "todo": "grep -HnR TODO index.js boot test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaelnisi/manger-http"
  },
  "keywords": [
    "feed",
    "cache",
    "http",
    "server"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelnisi/manger-http/issues"
  },
  "homepage": "https://github.com/michaelnisi/manger-http",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "bunyan": "^1.0.1",
    "http-hash-router": "^1.1.0",
    "manger": "^2.0.6",
    "mkdirp": "^0.5.0",
    "negotiator": "^0.5.3",
    "readable-stream": "^1.1.13"
  },
  "devDependencies": {
    "nock": "^2.9.1",
    "rimraf": "^2.4.1",
    "tap": "^1.1.4"
  },
  "engines": {
    "node": ">=0.10"
  },
  "author": "Michael Nisi <michael.nisi@gmail.com> (http://michaelnisi.com/)"
}
